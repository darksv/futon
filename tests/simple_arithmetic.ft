fn add(a: u32, b: u32) -> u32 {
    let c: u32 = a * b;
    let c2: u32 = a + b * c;
    let d: u32 = -a - b;
    let e = -a - b - c2;

    return c - a / d - b * e;
}

fn min(a: u32, b: u32, c: u32) -> u32 {
    if a < b {
        if a < c {
            return a;
        } else {
            return c;
        }
    } else {
        if b < c {
            return b;
        } else {
            return b;
        }
    }
}
